{
  "name": "react-localized",
  "version": "3.1.0",
  "description": "Internationalization for React components",
  "main": "lib/react-localized.js",
  "module": "lib/react-localized.module.js",
  "exports": "./lib/react-localized.modern.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "docs:react": "webpack --config webpack.config.docs.js",
    "dev:react": "webpack serve --config webpack.config.dev.js",
    "lint": "eslint src",
    "clean": "rimraf lib",
    "compile": "microbundle -i src -o lib -f modern,es,cjs --no-sourcemap",
    "build": "yarn run clean && yarn run compile",
    "prepublishOnly": "yarn run lint && yarn run build"
  },
  "keywords": [
    "react",
    "i18n",
    "localization",
    "gettext",
    "internationalization"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fakundo/react-localized"
  },
  "author": "Roman Samoylov (https://github.com/fakundo)",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "dependencies": {
    "react-localized-core": "^1.1.0"
  }
}
