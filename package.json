{
  "name": "react-localized",
  "version": "2.0.4",
  "description": "Internationalization for React and Preact components",
  "main": "lib/index.js",
  "files": [
    "lib",
    "messages-loader.js",
    "useReact.js",
    "usePreact.js"
  ],
  "scripts": {
    "dev": "webpack-dev-server --progress --open",
    "lint": "eslint src && eslint examples",
    "clean": "rimraf lib",
    "compile": "NODE_ENV=commonjs babel src -d lib",
    "build": "npm run clean && npm run compile",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "keywords": [
    "react",
    "preact",
    "i18n",
    "localization",
    "gettext",
    "internationalization"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fakundo/react-localized"
  },
  "author": "Roman Samoylov (https://github.com/fakundo)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "date-fns": "^1.30.1",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "html-webpack-plugin": "^3.2.0",
    "preact": "^8.5.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rimraf": "^3.0.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "lodash": "^4.17.11",
    "po2json": "^0.4.5"
  }
}
